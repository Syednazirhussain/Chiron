            } else if (meridiem === 'à¤¦à¤¿à¤à¤à¤¸à¥') {
                return hour >= 10 ? hour : hour + 12;
            } else if (meridiem === 'à¤¸à¤¾à¤à¤') {
                return hour + 12;
            }
        },
        meridiem: function (hour, minute, isLower) {
            if (hour < 3) {
                return 'à¤°à¤¾à¤¤à¤¿';
            } else if (hour < 12) {
                return 'à¤¬à¤¿à¤¹à¤¾à¤¨';
            } else if (hour < 16) {
                return 'à¤¦à¤¿à¤à¤à¤¸à¥';
            } else if (hour < 20) {
                return 'à¤¸à¤¾à¤à¤';
            } else {
                return 'à¤°à¤¾à¤¤à¤¿';
            }
        },
        calendar: {
            sameDay: '[à¤à¤] LT',
            nextDay: '[à¤­à¥à¤²à¤¿] LT',
            nextWeek: '[à¤à¤à¤à¤¦à¥] dddd[,] LT',
            lastDay: '[à¤¹à¤¿à¤à¥] LT',
            lastWeek: '[à¤à¤à¤à¥] dddd[,] LT',
            sameElse: 'L',
        },
        relativeTime: {
            future: '%sà¤®à¤¾',
            past: '%s à¤à¤à¤¾à¤¡à¤¿',
            s: 'à¤à¥à¤¹à¥ à¤à¥à¤·à¤£',
            ss: '%d à¤¸à¥à¤à¥à¤£à¥à¤¡',
            m: 'à¤à¤ à¤®à¤¿à¤¨à¥à¤',
            mm: '%d à¤®à¤¿à¤¨à¥à¤',
            h: 'à¤à¤ à¤à¤£à¥à¤à¤¾',
            hh: '%d à¤à¤£à¥à¤à¤¾',
            d: 'à¤à¤ à¤¦à¤¿à¤¨',
