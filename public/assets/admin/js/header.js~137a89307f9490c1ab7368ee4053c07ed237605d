            M: 'ÛÚ© ÙØ§Ù',
            MM: '%d ÙØ§Ù',
            y: 'ÛÚ© Ø³Ø§Ù',
            yy: '%d Ø³Ø§Ù',
        },
        preparse: function (string) {
            return string
                .replace(/[Û°-Û¹]/g, function (match) {
                    return numberMap[match];
                })
                .replace(/Ø/g, ',');
        },
        postformat: function (string) {
            return string
                .replace(/\d/g, function (match) {
                    return symbolMap[match];
                })
                .replace(/,/g, 'Ø');
        },
        dayOfMonthOrdinalParse: /\d{1,2}Ù/,
        ordinal: '%dÙ',
