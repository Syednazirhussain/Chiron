                    case 0:
                        return '[proÅ¡lu] [nedjelju] [u] LT';
                    case 3:
                        return '[proÅ¡lu] [srijedu] [u] LT';
                    case 6:
                        return '[proÅ¡le] [subote] [u] LT';
                    case 1:
