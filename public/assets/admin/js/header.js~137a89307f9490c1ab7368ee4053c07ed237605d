        m: 'minÅ«tes_minÅ«tÄm_minÅ«te_minÅ«tes'.split('_'),
        mm: 'minÅ«tes_minÅ«tÄm_minÅ«te_minÅ«tes'.split('_'),
        h: 'stundas_stundÄm_stunda_stundas'.split('_'),
